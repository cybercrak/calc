<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@+id/blankFragment">
    <fragment
        android:id="@+id/blankFragment"
        android:label="fragment_blank"
        android:name="com.example.a1stapp.BlankFragment"
        tools:layout="@layout/fragment_blank" >


        <action
            android:id="@+id/action_blankFragment_to_mainActivity2"
            app:destination="@+id/mainActivity2" />
        <action
            android:id="@+id/action_blankFragment_to_aboutActivity"
            app:destination="@+id/aboutActivity" />
        <action
            android:id="@+id/action_blankFragment_to_contactActivity"
            app:destination="@+id/contactActivity" />
    </fragment>
    <activity
        android:id="@+id/mainActivity2"
        android:name="com.example.a1stapp.MainActivity2"
        android:label="activity_main2"
        tools:layout="@layout/activity_main2" />
    <activity
        android:id="@+id/aboutActivity"
        android:name="com.example.a1stapp.aboutActivity"
        android:label="activity_about"
        tools:layout="@layout/activity_about"/>
    <activity
        android:id="@+id/contactActivity"
        android:name="com.example.a1stapp.contactActivity"
        android:label="activity_contact"
        tools:layout="@layout/activity_contact" />
</navigation>